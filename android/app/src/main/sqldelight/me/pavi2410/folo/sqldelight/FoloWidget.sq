CREATE TABLE FoloWidget (
  id INTEGER PRIMARY KEY,
  profileId INTEGER NOT NULL,
  FOREIGN KEY (profileId) REFERENCES FoloProfile (id) ON DELETE CASCADE
);

selectAll:
SELECT *
FROM FoloWidget;

selectById:
SELECT *
FROM FoloWidget
WHERE id = ?;

insertWidget:
INSERT INTO FoloWidget(id, profileId)
VALUES (?, ?);
