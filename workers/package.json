{
	"name": "workers",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"db:generate": "drizzle-kit generate",
		"db:migrate:remote": "wrangler d1 migrations apply folo-db --remote",
		"db:migrate:local": "wrangler d1 migrations apply folo-db --local",
		"db:seed:remote": "wrangler d1 execute folo-db --remote --file=seed.sql",
		"db:seed:local": "wrangler d1 execute folo-db --local --file=seed.sql"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250815.0",
		"@types/node": "^24.3.0",
		"drizzle-kit": "^0.31.4",
		"typescript": "^5.2.2",
		"wrangler": "^4.31.0"
	},
	"dependencies": {
		"@hono/zod-validator": "^0.7.2",
		"drizzle-orm": "^0.44.4",
		"hono": "^4.9.2",
		"zod": "^4.0.17"
	}
}
